{
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "linebreak-style": 0,
    "arrow-body-style": 0,
    "react/prop-types": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "func-names": 0,
    "prefer-destructuring": 0,
    "no-nested-ternary": 0,
    "react/forbid-prop-types": 0,
    "max-len": 0,
    "import/prefer-default-export": 0
  }
}
